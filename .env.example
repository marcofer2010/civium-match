# Configurações básicas
DEBUG=true
LOG_LEVEL=INFO

# Servidor
HOST=0.0.0.0
PORT=8002

# FAISS Configuration
FAISS_DATA_DIR=data
FAISS_INDEX_TYPE=IVF1024,Flat

# Parâmetros de match
DEFAULT_MATCH_THRESHOLD=0.4
DEFAULT_TOP_K=10
EMBEDDING_DIMENSION=512

# Performance
MAX_BATCH_SIZE=100

# Cache
ENABLE_CACHE=true
CACHE_TTL=3600

# Monitoramento
ENABLE_METRICS=true

# Security
# SECRET_KEY=your-secret-key-here
# API_TOKEN=your-api-token-here

# CORS
ALLOWED_ORIGINS=["http://localhost:3000", "http://localhost:8000"] 